{
  "name": "snapcut-beat-editor",
  "private": true,
  "version": "1.0.0",
  "description": "Beat-synced video template editor for Snapcut",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prebuild": "node scripts/fetch-ffmpeg-core.mjs public",
    "postinstall": "node scripts/fetch-ffmpeg-core.mjs public",
    "postbuild": "node scripts/fetch-ffmpeg-core.mjs dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:analyze": "vite build --mode analyze",
    "deploy": "npm run build && echo 'Build completed! Ready for Netlify deployment.'",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
